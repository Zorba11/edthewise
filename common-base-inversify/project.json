{
  "name": "common-base-inversify",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "common-base-inversify/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/common-base-inversify",
        "main": "common-base-inversify/src/index.ts",
        "tsConfig": "common-base-inversify/tsconfig.lib.json",
        "assets": ["common-base-inversify/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["common-base-inversify/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "common-base-inversify/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
